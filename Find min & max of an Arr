#include <iostream>
using namespace std;


int main()
{
    int size=6;
    int Arr[size]={1,8,3,4,5,6};

    int Min= Arr[0];
    int Max= Arr[0];


    for (int i=0;i<size;i++)
    {
        if (Arr[i]<Min)
        {
            Min=Arr[i];

        }

    }

    for (int i=0;i<size;i++)
    {

        if (Arr[i]>Max)
        {
            Max=Arr[i];

        }
    }

    cout<<Min<<endl;
    cout<<Max;



return 0;
}
